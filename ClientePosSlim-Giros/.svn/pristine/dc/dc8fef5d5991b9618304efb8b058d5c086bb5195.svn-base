package co.com.codesa.clienteposslimgiros.dto.xml.actividadesEconomicas;

import java.util.ArrayList;
import java.util.List;

import co.com.codesa.codesawrappergiros.modelo.dominio.giros.negocio.ActividadEconomica;

public class actividadesEconomicasXMLDTO {

	private List<ActividadEconomica> lstActividadesEconomicas;
	
	public actividadesEconomicasXMLDTO(){
		
		this.lstActividadesEconomicas = new ArrayList<ActividadEconomica>();
	}
	
	public void adicionarActividadEconomica(ActividadEconomica pObjActividadEconomica)throws Exception{
		
		this.lstActividadesEconomicas.add(pObjActividadEconomica);
	}
}
